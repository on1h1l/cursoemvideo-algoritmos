algoritmo "ex004"
var
   n1, n2, total: inteiro
inicio
      escreva("Digite um número inteiro:")
      leia(n1)
      escreva("Digite outro número inteiro:")
      leia(n2)
      
      total <- n1+n2
      
      limpatela
      escreva("A soma entre",n1," e",n2," é igual a",total)
fimalgoritmo
