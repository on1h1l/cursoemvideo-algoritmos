algoritmo "ex003"
var
   nome_colab: caractere
   sal_colab: real
inicio
      escreva("Nome do funcionário:")
      leia(nome_colab)
      escreva("Salário do funcionário: R$")
      leia(sal_colab)
      limpatela
      escreva("O funcionário ",nome_colab," recebeu R$",sal_colab," esse mês.")
fimalgoritmo
