algoritmo "ex011"
var
   a, b, c, delta: real
inicio
      escreva("Digite o valor de A:")
      leia(a)
      escreva("Digite o valor de B:")
      leia(b)
      escreva("Digite o valor de C:")
      leia(c)
      limpaTela
      delta <- (b)^2-4*a*c
      escrevaL("A:",a,"  B:",b,"  C:",c)
      escreva("Delta:",delta)
fimalgoritmo
